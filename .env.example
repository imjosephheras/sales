# Database Configuration
# Copy this file to .env and update with your credentials:
#   cp .env.example .env
DB_HOST=localhost
DB_NAME=form
DB_USER=root
DB_PASS=
DB_CHARSET=utf8mb4

# Storage Configuration
# Set STORAGE_DISK=ftp to upload files to remote FTP server
# Set STORAGE_DISK=local to store files locally (default)
STORAGE_DISK=local

# FTP Server Configuration (only used when STORAGE_DISK=ftp)
FTP_HOST=
FTP_PORT=21
FTP_USERNAME=
FTP_PASSWORD=
FTP_ROOT=/
FTP_PASSIVE=true
